<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="DinoFencesDoorBase">
    <defName>BathroomDoor</defName>
    <label>bathroom door</label>
    <description>A door designed to fit with bathroom blocks. Can be entirely functional or aesthetic depending on what mods are installed.
	
When you gotta go, you gotta go.</description>
    <fillPercent>0.15</fillPercent>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>500</WorkToBuild>
      <Flammability>0</Flammability>
    </statBases>
    <stuffCategories>
      <li>Woody</li>
      <li>Stony</li>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>10</costStuffCount>
    <holdsRoof>false</holdsRoof>
    <graphicData>
      <texPath>Things/Building/Aviary/BathroomDoor_Mover</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <linkFlags>
        <li>Custom2</li>
      </linkFlags>
      <damageData>
        <enabled>false</enabled>
      </damageData>
    </graphicData>
    <designationCategory>AviaryJurassicRimworld</designationCategory>
    <uiIconPath>Things/Building/Aviary/BathroomDoorIco</uiIconPath>
    <damageMultipliers>
      <li>
        <damageDef>Bomb</damageDef>
        <multiplier>3</multiplier>
      </li>
    </damageMultipliers>
  </ThingDef>
