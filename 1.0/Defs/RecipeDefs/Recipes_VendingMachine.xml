<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>VMCoprolite</defName>
    <label>buy some Coprolite Rocks</label>
    <description>Buy some Coprolite rocks from a vending machine.</description>
    <jobString>buying snack.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_MakeStoneBlocks</soundWorking>
    <workAmount>50</workAmount>
    <workSkill>Crafting</workSkill>
    <workSkillLearnFactor>0</workSkillLearnFactor>
    <recipeUsers>
      <li>DinoVendingMachineFood</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Silver</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <CoproliteRocks>5</CoproliteRocks>
    </products>
  </RecipeDef>
