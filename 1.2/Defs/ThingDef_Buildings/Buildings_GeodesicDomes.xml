<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>GeodesicDomeSmall</defName>
    <label>small geodesic dome</label>
    <description>A small dome with a tesselated framework intersected with panes of glass.</description>
    <category>Building</category>
    <graphicData>
      <texPath>Things/Building/Aviary/GeodesicDomeSmall</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>Transparent</shaderType>
      <drawRotated>false</drawRotated>
      <drawSize>(10,10)</drawSize>
      <damageData>
      </damageData>
    </graphicData>
    <rotatable>false</rotatable>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <passability>Standable</passability>
    <canOverlapZones>true</canOverlapZones>
    <pathCost>8</pathCost>
    <tickerType>Normal</tickerType>
    <size>(2,2)</size>
    <drawerType>RealtimeOnly</drawerType>
    <fillPercent>0.15</fillPercent>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>100</WorkToBuild>
      <Mass>5</Mass>
      <Flammability>0</Flammability>
      <Beauty>5</Beauty>
    </statBases>
    <selectable>true</selectable>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>2</costStuffCount>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <resourcesFractionWhenDeconstructed>0.5</resourcesFractionWhenDeconstructed>
    <placeWorkers>
      <li>PlaceWorker_Heater</li>
    </placeWorkers>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <designationCategory>AviaryJurassicRimworld</designationCategory>
    <uiIconScale>0.15</uiIconScale>
    <researchPrerequisites>
      <li>JurassicFurniture</li>
    </researchPrerequisites>
  </ThingDef>

