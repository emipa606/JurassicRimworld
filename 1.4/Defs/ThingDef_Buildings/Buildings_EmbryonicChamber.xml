<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>EmbryonicChamber</defName>
    <label>embryonic chamber</label>
    <graphicData>
      <texPath>Things/Building/EmbryonicChamber</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1,3)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.35</staticSunShadowHeight>
    <fillPercent>0.5</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>1800</WorkToBuild>
      <Mass>40</Mass>
      <Flammability>1.0</Flammability>
    </statBases>
    <description>An embryonic chamber that holds valuable inforamation. Building one near a DNA reconstruction or amber extraction bench will give a boon to their work speed. Placing more than one has no effect.</description>
    <size>(1,1)</size>
    <costList>
      <Amber>5</Amber>
      <ComponentIndustrial>5</ComponentIndustrial>
      <Steel>50</Steel>
    </costList>
    <comps>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <WorkTableWorkSpeedFactor>0.10</WorkTableWorkSpeedFactor>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
      </li>
    </comps>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
    </placeWorkers>
    <designationCategory>JurassicRimworld</designationCategory>
    <building>
      <soundAmbient>EmbryonicChamber_Ambience</soundAmbient>
    </building>
  </ThingDef>
</Defs>