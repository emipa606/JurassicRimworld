<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="FurnitureWithQualityBase">
    <defName>Holoscape</defName>
    <label>holoscape</label>
    <description>A holographic table that displays 3D projections. Colonists can learn about and interact with life-like, tiny dinosaurs. Isn't that neat!

Using this table gives experience in the intellectual skill. Needs chairs!</description>
    <graphicData>
      <texPath>Things/Building/Holoscape</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(4,4)</drawSize>
    </graphicData>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <altitudeLayer>Building</altitudeLayer>
    <socialPropernessMatters>true</socialPropernessMatters>
    <fillPercent>0.40</fillPercent>
    <rotatable>false</rotatable>
    <building>
      <joyKind>Gaming_Cerebral</joyKind>
    </building>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>12000</WorkToBuild>
      <Mass>120</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>2</Beauty>
      <JoyGainFactor>1.3</JoyGainFactor>
    </statBases>
    <size>(2,2)</size>
    <costList>
      <Steel>40</Steel>
      <ComponentIndustrial>8</ComponentIndustrial>
    </costList>
    <pathCost>70</pathCost>
    <passability>PassThroughOnly</passability>
    <designationCategory>JurassicRimworld</designationCategory>
    <canOverlapZones>false</canOverlapZones>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>170</basePowerConsumption>
        <shortCircuitInRain>true</shortCircuitInRain>
      </li>
      <li Class="CompProperties_Flickable" />
      <li Class="CompProperties_Breakdownable" />
    </comps>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
  </ThingDef>
</Defs>