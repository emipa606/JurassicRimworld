<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BenchBase">
    <defName>DinoVendingMachineDrink</defName>
    <label>drink vending machine</label>
    <thingClass>Building_WorkTable</thingClass>
    <description>A vending machine that dispenses drinks.</description>
    <graphicData>
      <texPath>Things/Building/VendingMachineDrink</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <damageData>
        <rect>(0.05,0.1,0.9,0.9)</rect>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
      </damageData>
      <drawSize>(2.5,2.5)</drawSize>
    </graphicData>
    <rotatable>false</rotatable>
    <costList>
      <Steel>100</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>1500</WorkToBuild>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <designationCategory>JurassicRimworld</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
    <comps>
      <li Class="CompProperties_Glower">
        <glowRadius>3</glowRadius>
        <glowColor>(216, 255, 253)</glowColor>
      </li>
      <li Class="CompProperties_Breakdownable" />
    </comps>
    <placeWorkers>
      <li>PlaceWorker_Heater</li>
    </placeWorkers>
    <uiIconScale>0.25</uiIconScale>
    <researchPrerequisites>
      <li>JurassicFurniture</li>
    </researchPrerequisites>
  </ThingDef>

